<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="VEGASTAR.App">

    <Application.Styles>
        <FluentTheme Mode="Light" />
        <Style Selector="TextBlock">
            <Setter Property="FontFamily" Value="avares://VEGASTAR/Assets/Fonts/Roboto-Light.ttf#Roboto" />
        </Style>
        <Style Selector="TextBlock.Awesome">
            <Setter Property="FontFamily" Value="avares://VEGASTAR/Assets/Fonts/Font Awesome 6 Free-Regular-400.otf" />
        </Style>
        <StyleInclude Source="avares://VEGASTAR/Themes/Buttons.axaml" />
        <StyleInclude Source="avares://MshaControls/Controls/MonthChecked.axaml" />
        <StyleInclude Source="avares://MshaControls/Controls/YearsChecked.axaml" />
        <StyleInclude Source="avares://MshaControls/HamburgerMenu/HamburgerMenu.axaml" />
    </Application.Styles>
</Application>